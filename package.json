{
  "name": "formkit-multistep",
  "version": "0.0.11",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totalhack/formkit-multistep.git"
  },
  "keywords": [
    "formkit",
    "multistep",
    "vue form"
  ],
  "author": "totalhack",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/formkit-multistep.umd.js",
  "module": "./dist/formkit-multistep.es.js",
  "exports": {
    ".": {
      "import": "./dist/formkit-multistep.es.js",
      "require": "./dist/formkit-multistep.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "pub": "vite build && npm publish",
    "patch": "npm version patch --force --commit-hooks false",
    "patchpub": "vite build && npm version patch --force --commit-hooks false && npm publish"
  },
  "dependencies": {
    "@formkit/tailwindcss": "^1.0.0-beta.8",
    "@formkit/vue": "^1.0.0-beta.8",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "vite": "^2.6.4",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24"
  }
}